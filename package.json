{
  "name": "angular-express",
  "version": "1.0.0",
  "description": "test",
  "scripts": {
    "ng": "npm link @angular/cli",
    "install:client": "cd client && npm install",
    "install:server": "cd server && npm install",
    "install:packages": "npm install && npm run install:client && npm run install:server",
    "build:client": "cd client && ng build --aot=true --base-href=/ --output-path=../dist/client",
    "build:server": "cd server &&  npx tsc",
    "build": "npm run build:client && npm run build:server",
    "start": "node --optimize_for_size --max_old_space_size=920 ./dist/app.js ",
    "start:dev" : "nodemon server/app.ts && cd ./client && ng serve "
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/forms": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "bootstrap": "^5.2.1",
    "concurrently": "^3.5.0",
    "cors": "^2.8.5",
    "cpx": "^1.5.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "node-cache": "^5.1.2",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "rxjs": "~7.5.6",
    "sequelize": "^6.21.6",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular/cli": "~14.2.2",
    "@angular/compiler-cli": "^14.2.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.16",
    "@types/pg": "^8.6.5",
    "eslint": "^8.23.0",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3"
  }
